{
    "manifest_version": 3,
    "default_locale": "en",

    "name": "__MSG_extension_name__",
    "description": "__MSG_extension_description__",
    "version": "1.0",

    "icons": {
        "48": "images/icon-48 2.png",
        "96": "images/icon-96 2.png",
        "128": "images/icon-128 2.png",
        "256": "images/icon-256 2.png",
        "512": "images/icon-512.png"
    },

    "background": {
        "service_worker": "background.js"
    },

    "permissions": [
        "tabs",
        "scripting"
    ],

    "action": {
        "default_icon": "images/default icon.svg"
    },
    "commands": {
        "jump-audible": {
          "description": "Go to the tab that's playing audio"
        }
      }
}
